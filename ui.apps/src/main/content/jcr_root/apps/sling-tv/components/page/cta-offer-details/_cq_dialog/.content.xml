<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Content"
    sling:resourceType="cq/gui/components/authoring/dialog"
    mode="edit">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container"
        class="cq-dialog-content-page">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container"
                rel="cq-siteadmin-admin-properties-tabs">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/tabs"
                    type="nav"/>
                <items jcr:primaryType="nt:unstructured">
                    <navigation
                        cq:hideOnEdit="{Boolean}false"
                        cq:showOnCreate="{Boolean}true"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Category Content Creation"
                        sling:resourceType="granite/ui/components/foundation/section">
                        <layout
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                            margin="{Boolean}false"/>
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                        cq:hideOnEdit="{Boolean}false"
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:description="This is just the name of the Page"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        cq-msm-lockable="jcr:title"
                                        fieldLabel="Page Name"
                                        name="./jcr:title"
                                        required="{Boolean}true"
                                        useFixedInlineToolbar="{Boolean}true"/>
                                    <offerDetailsLabel
                                        cq:hideOnEdit="{Boolean}false"
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldDescription="Please Enter the description of Offer Details to show Offer Details Link"
                                        fieldLabel="Offer Details Label"
                                        name="./offerDetailsLabel"/>
                                    <publishedDate
                                        cq:hideOnEdit="{Boolean}false"
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        defaultValue="Updated Monday 20 , 2017"
                                        fieldLabel="Published Date "
                                        name="./offerPublishedDate"/>
                                    <offerDetailsHeadline
                                        cq:hideOnEdit="{Boolean}false"
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldLabel="Offer Details Heading"
                                        name="./offerDetailsHeading"/>
                                    <offerDetailsDateAttr
                                        cq:hideOnEdit="{Boolean}false"
                                        cq:showOnCreate="{Boolean}true"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                                        fieldDescription="Configure the date attribute For ex: 7"
                                        fieldLabel="Offer Details Date Attr"
                                        name="./offerDetailsDateAttr"/>
                                </items>
                            </column>
                        </items>
                    </navigation>
                    <basic
                        cq:hideOnEdit="{Boolean}true"
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="basic"
                        sling:resourceType="granite/ui/components/foundation/section"/>
                    <advanced
                        cq:hideOnEdit="{Boolean}true"
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="basic"
                        sling:resourceType="granite/ui/components/foundation/section"/>
                    <thumbnail
                        cq:hideOnEdit="{Boolean}true"
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="basic"
                        sling:resourceType="granite/ui/components/foundation/section"/>
                    <cloudservices
                        cq:hideOnEdit="{Boolean}true"
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="basic"
                        sling:resourceType="granite/ui/components/foundation/section"/>
                    <personalization
                        cq:hideOnEdit="{Boolean}true"
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="basic"
                        sling:resourceType="granite/ui/components/foundation/section"/>
                    <permissions
                        cq:hideOnEdit="{Boolean}true"
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="basic"
                        sling:resourceType="granite/ui/components/foundation/section"/>
                    <blueprint
                        cq:hideOnEdit="{Boolean}true"
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="basic"
                        sling:resourceType="granite/ui/components/foundation/section"/>
                    <livecopy
                        cq:hideOnEdit="{Boolean}true"
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="basic"
                        sling:resourceType="granite/ui/components/foundation/section"/>
                    <icon
                        cq:hideOnEdit="{Boolean}true"
                        cq:showOnCreate="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Icon"
                        sling:resourceType="granite/ui/components/foundation/section"/>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
