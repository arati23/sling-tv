<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:language="es"
    jcr:mixinTypes="[mix:language]"
    jcr:primaryType="sling:Folder">
    <navigation-page-link
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="ui.navigation.page.link"
        sling:message="Enlazar"/>
    <navigation-page-name
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="ui.navigation.page.name"
        sling:message="Nombre"/>
    <navigation-page-tab
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="ui.navigation.page.tab"
        sling:message="Ficha de la página de navegación"/>
    <navigation-page-title
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="ui.navigation.page.title"
        sling:message="Título"/>    
    <mo
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="mo"
        sling:message="mes"/>
    <MORE
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="MORE"
        sling:message="MÁS"/>    
    <LESS
        jcr:mixinTypes="[sling:Message]"
        jcr:primaryType="nt:folder"
        sling:key="LESS"
        sling:message="MENOS"/>    
</jcr:root>
